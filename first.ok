import std.io;

func main(args:str[]) -> int is
    var x : float := 99.12300;
    var y : float := 2.398100;
    
    printFloat(x);
    printFloat(y);
    println("---------------");
    
    var a1 : float := x + y;
    var a2 : float := x - y;
    var a3 : float := x * y;
    var a4 : float := x / y;
    
    printFloat(a1);
    printFloat(a2);
    printFloat(a3);
    printFloat(a4);
    
    return 0;
end
