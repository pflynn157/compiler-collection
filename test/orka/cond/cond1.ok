import std.io;

func compare(x:int) is
    if x > 10 then
        println("Greater than 10");
        if x > 15 then
            println("Greater than 15");
        else
            println("Less than 15");
        end
        println("Done");
    else
        println("Less");
    end
end

func main -> int is
    var x : int := 40;
    
    compare(x);
    println("---");
    compare(3);
    println("---");
    compare(11);
    println("---");
    
    return 0;
end

