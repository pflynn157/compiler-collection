import std.io;

func main(args:str[]) -> int is
    var x : double := 99.12300;
    var y : double := 2.398100;
    
    printDouble(x);
    printDouble(y);
    println("---------------");
    
    var a1 : double := x + y;
    var a2 : double := x - y;
    var a3 : double := x * y;
    var a4 : double := x / y;
    
    printDouble(a1);
    printDouble(a2);
    printDouble(a3);
    printDouble(a4);
    
    return 0;
end
