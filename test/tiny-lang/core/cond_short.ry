
func test1(x:i16) -> i32 is
    if x = 20 then return 1;
    else return 2;
    end
    
    return 0;
end

func test2(x:i16) -> i32 is
    if x = 20 then return 1;
    else return 2;
    end
    
    return 0;
end

func main -> i32 is
    var x1 : i16 := 20;
    var x2 : i16 := 31;
    var ret : i32 := 0;
    
    ret := ret + test1(x1);
    ret := ret + test2(x2);
    return 3 - ret;
end
